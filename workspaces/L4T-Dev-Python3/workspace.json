{
  "description": "Pyhton3 Development Environment",
  "docker_registry": "https://docker2.newey.id.au/v2/", 
  "image_src": "ubuntuwsp3.png",  
  "name": "docker2.newey.id.au/jetson/l4t-python-dev:alpha1", 
  "run_config": {
    "hostname": "Jetson-Kasm-{{user.username}}"
  },
  "exec_config": {
    "go": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --go --url \"$KASM_URL\"'"
    },
    "assign": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
    }
  },
  "categories": [
    "Workstation" 
  ],
  "friendly_name": "L4T Python 3" ,
  "architecture": [
    "arm64"
  ],
  "compatibility": [
    "1.13.x",
    "1.14.x"
  ],
  "uncompressed_size_mb": 7000
}
